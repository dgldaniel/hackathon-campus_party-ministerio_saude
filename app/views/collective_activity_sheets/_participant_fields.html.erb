<div class="nested-fields">
  <h3>Participant</h3>
  <div class="row">
    <div class="col">
      <div class="mb-3">
        <%= f.label :cnsParticipante %>
        <%= f.text_field :cnsParticipante, class: 'form-control btn-pill' %>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col">
      <div class="mb-3">
        <%= f.label :dataNascimento %>
        <div class="input-group">
          <%= f.text_field :dataNascimento, class: 'btn-pill datepicker-here form-control digits', 'data-language': "en", 'data-bs-original-title': 'data-bs-original-title', title: 'title' %>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col">
      <div class="mb-3">
        <%= f.check_box :avaliacaoAlterada %>
        <%= f.label :avaliacaoAlterada %>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col">
      <div class="mb-3">
        <%= f.check_box :cessouHabitoFumar %>
        <%= f.label :cessouHabitoFumar %>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col">
      <div class="mb-3">
        <%= f.check_box :abandonouGrupo %>
        <%= f.label :abandonouGrupo %>
      </div>
    </div>
  </div>
  <%= link_to_remove_association "remove participant", f %>
  <br>
  <br>
  <br>
</div>
