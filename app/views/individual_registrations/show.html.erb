<button class="js-modal-trigger" data-target="modal-js-example">
  Enviar Mensagem para o Paciente
</button>

<div id="modal-js-example" class="modal">
  <div class="modal-background"></div>

  <div class="modal-content">
    <div class="box">
     <%= form_with(url: notify_patient_path(id: @individual_registration.id), local: false, html: { autocomplete: 'off' }) do |form| %>
     <h2>Enviar Mensagem para o paciente: <%= @individual_registration.nome_completo %>
      <div class="field">
        <%= form.label :title %>
        <%= form.text_field :title %>
      </div>

      <div class="field">
        <%= form.label :description %>
        <%= form.text_field :description %>
      </div>

      <div class="actions">
        <%= form.submit %>
      </div>
     <% end %>
    </div>
  </div>

  <button class="modal-close is-large" aria-label="close"></button>
</div>
